<form class="form-signin" action="/users/forgetPassword?email=<%= email %>" method="POST">
  <h2 class="form-signin-heading">Please Enter your Email</h2>

  <label for="inputEmail" class="sr-only">Email address</label>
  <input type="email" id="inputEmail" name="email" class="form-control" placeholder="Email address" required autofocus>
  
  <button class="btn btn-lg btn-default btn-block" type="submit">Submit</button>
</form>
